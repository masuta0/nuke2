entrypoint = "index.js"
modules = ["nodejs-22"]
hidden = [".config", "package-lock.json"]
language = "nodejs"
[gitHubImport]
requiredFiles = [".replit", "replit.nix", "package.json", "package-lock.json"]

[nix]
channel = "stable-24_11"

[deployment]
nodejs_version = "18.x"
run = "bash start.sh"
run = ["node", "index.js"]
deploymentTarget = "autoscale"
ignorePorts = false

[[ports]]
localPort = 3000
externalPort = 80
